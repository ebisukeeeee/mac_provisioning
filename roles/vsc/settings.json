{
    "window.zoomLevel": 1,
    "workbench.editor.enablePreview": false,
    "breadcrumbs.enabled": true,
    "files.insertFinalNewline": true,
    "files.associations": {
        "*.erb": "erb"
    },
    "terminal.integrated.shell.osx": "/bin/zsh",
    "editor.renderWhitespace": "all",
    "editor.renderControlCharacters": true,
    "editor.minimap.enabled": true,
    "editor.formatOnSave": true,
    "editor.suggestSelection": "first",
    "editor.insertSpaces": true,
    "editor.showUnused": true,
    "editor.rulers": [
        80
    ],
    "editor.codeActionsOnSave": {
        "source.fixAll.eslint": true
    },
    "emmet.triggerExpansionOnTab": true,
    "emmet.includeLanguages": {
        "erb": "html"
    },
    "[javascript]": {
        "editor.tabSize": 2,
    },
    "javascript.implicitProjectConfig.experimentalDecorators": true,
    "[json]": {
        "editor.tabSize": 2,
    },
    "[go]": {
        "editor.formatOnSave": true,
        "editor.tabSize": 4,
    },
    "go.gocodeAutoBuild": true,
    "[typescript]": {
        "editor.formatOnPaste": true,
        "editor.formatOnSave": true,
        "editor.tabSize": 2,
    },
    "[ruby]": {
        "editor.tabSize": 2,
    },
    "ruby.lint": {
        "rubocop": true
    },
    "ruby.intellisense": "rubyLocate",
    "ruby.useLanguageServer": true,
    "ruby.codeCompletion": "rcodetools",
    "ruby.rubocop.executePath": "",
    "ruby.rubocop.suppressRubocopWarnings": true,
    "[html]": {
        "editor.tabSize": 2,
    },
    "[vue]": {
        "editor.formatOnSave": false,
        "editor.tabSize": 2,
    },
    "[erb]": {
        "editor.tabSize": 2,
    },
    "python.autoComplete.addBrackets": true,
    "python.pythonPath": "python3",
    "[dart]": {
        "editor.formatOnSave": true,
        "editor.formatOnType": true,
        "editor.rulers": [
            80
        ],
        "editor.selectionHighlight": false,
        "editor.suggest.snippetsPreventQuickSuggestions": false,
        "editor.suggestSelection": "first",
        "editor.tabCompletion": "onlySnippets",
        "editor.wordBasedSuggestions": false
    },
    "dart.flutterCreateAndroidLanguage": "kotlin",
    "dart.flutterCreateIOSLanguage": "swift",
    "solargraph.definitions": true,
    "solargraph.diagnostics": true,
    "solargraph.formatting": true,
    "explorer.confirmDragAndDrop": false,
    "plantuml.exportFormat": "png",
    "plantuml.jar": "",
    "eslint.alwaysShowStatus": true,
    "eslint.validate": [
        "javascript",
        {
            "language": "vue",
            "autoFix": true
        }
    ],
    "eslint.format.enable": true,
    "plantuml.render": "Local",
    "plantuml.urlFormat": "png",
    "plantuml.server": "http://localhost:7777/uml",
    "vetur.format.defaultFormatter.js": "none",
    "vetur.useWorkspaceDependencies": true,
    "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
    "hediet.vscode-drawio.local-storage": "",
}
